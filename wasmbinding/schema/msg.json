{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/neutron-org/neutron/v4/wasmbinding/bindings/neutron-msg",
  "$ref": "#/$defs/NeutronMsg",
  "$defs": {
    "AddSchedule": {
      "properties": {
        "name": {
          "type": "string"
        },
        "period": {
          "type": "integer"
        },
        "msgs": {
          "items": {
            "$ref": "#/$defs/MsgExecuteContract"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "name",
        "period",
        "msgs"
      ],
      "description": "AddSchedule adds new schedule to the cron module"
    },
    "AdminProposal": {
      "properties": {
        "param_change_proposal": {
          "$ref": "#/$defs/ParamChangeProposal"
        },
        "proposal_execute_message": {
          "$ref": "#/$defs/ProposalExecuteMessage"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "BurnTokens": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "amount": {
          "$ref": "#/$defs/Int"
        },
        "burn_from_address": {
          "type": "string",
          "description": "BurnFromAddress must be set to \"\" for now."
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "denom",
        "amount",
        "burn_from_address"
      ]
    },
    "ChangeAdmin": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "new_admin_address": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "denom",
        "new_admin_address"
      ],
      "description": "ChangeAdmin changes the admin for a factory denom."
    },
    "Coin": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "amount": {
          "$ref": "#/$defs/Int"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "amount"
      ]
    },
    "Coins": {
      "items": {
        "$ref": "#/$defs/Coin"
      },
      "type": "array"
    },
    "CreateDenom": {
      "properties": {
        "subdenom": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "subdenom"
      ],
      "description": "CreateDenom creates a new factory denom, of denomination: factory/{creating contract address}/{Subdenom} Subdenom can be of length at most 44 characters, in [0-9a-zA-Z./] The (creating contract address, subdenom) pair must be unique."
    },
    "DenomUnit": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "exponent": {
          "type": "integer"
        },
        "aliases": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "DepositOptions": {
      "properties": {
        "disable_autoswap": {
          "type": "boolean"
        },
        "fail_tx_on_bel": {
          "type": "boolean"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "Dex": {
      "properties": {
        "deposit": {
          "$ref": "#/$defs/MsgDeposit"
        },
        "withdrawal": {
          "$ref": "#/$defs/MsgWithdrawal"
        },
        "place_limit_order": {
          "$ref": "#/$defs/MsgPlaceLimitOrder"
        },
        "withdraw_filled_limit_order": {
          "$ref": "#/$defs/MsgWithdrawFilledLimitOrder"
        },
        "cancel_limit_order": {
          "$ref": "#/$defs/MsgCancelLimitOrder"
        },
        "multi_hop_swap": {
          "$ref": "#/$defs/MsgMultiHopSwap"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "deposit",
        "withdrawal",
        "place_limit_order",
        "withdraw_filled_limit_order",
        "cancel_limit_order",
        "multi_hop_swap"
      ]
    },
    "Fee": {
      "properties": {
        "recv_fee": {
          "$ref": "#/$defs/Coins",
          "description": "the packet receive fee"
        },
        "ack_fee": {
          "$ref": "#/$defs/Coins",
          "description": "the packet acknowledgement fee"
        },
        "timeout_fee": {
          "$ref": "#/$defs/Coins",
          "description": "the packet timeout fee"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "recv_fee",
        "ack_fee",
        "timeout_fee"
      ],
      "description": "Fee defines the ICS29 receive, acknowledgement and timeout fees"
    },
    "ForceTransfer": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "amount": {
          "$ref": "#/$defs/Int"
        },
        "transfer_from_address": {
          "type": "string"
        },
        "transfer_to_address": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "denom",
        "amount",
        "transfer_from_address",
        "transfer_to_address"
      ],
      "description": "ForceTransfer forces transferring of a specific denom is only allowed for the creator of the denom registered during CreateDenom."
    },
    "Height": {
      "properties": {
        "revision_number": {
          "type": "integer"
        },
        "revision_height": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "Int": {
      "properties": {},
      "additionalProperties": false,
      "type": "object"
    },
    "KVKey": {
      "properties": {
        "path": {
          "type": "string",
          "description": "Path (storage prefix) to the storage where you want to read value by key\n(usually name of cosmos-sdk module: 'staking', 'bank', etc.)"
        },
        "key": {
          "type": "string",
          "contentEncoding": "base64",
          "description": "Key you want to read from the storage"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "MintTokens": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "amount": {
          "$ref": "#/$defs/Int"
        },
        "mint_to_address": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "denom",
        "amount",
        "mint_to_address"
      ]
    },
    "MsgCancelLimitOrder": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "tranche_key": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "MsgDeposit": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "receiver": {
          "type": "string"
        },
        "token_a": {
          "type": "string"
        },
        "token_b": {
          "type": "string"
        },
        "amounts_a": {
          "items": {
            "$ref": "#/$defs/Int"
          },
          "type": "array"
        },
        "amounts_b": {
          "items": {
            "$ref": "#/$defs/Int"
          },
          "type": "array"
        },
        "tick_indexes_a_to_b": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "fees": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "options": {
          "items": {
            "$ref": "#/$defs/DepositOptions"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "amounts_a",
        "amounts_b"
      ]
    },
    "MsgExecuteContract": {
      "properties": {
        "contract": {
          "type": "string",
          "description": "Contract is the address of the smart contract"
        },
        "msg": {
          "type": "string",
          "description": "Msg json encoded message to be passed to the contract"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "description": "MsgExecuteContract defined separate from wasmtypes since we can get away with just passing the string into bindings"
    },
    "MsgMultiHopSwap": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "receiver": {
          "type": "string"
        },
        "routes": {
          "items": {
            "$ref": "#/$defs/MultiHopRoute"
          },
          "type": "array"
        },
        "amount_in": {
          "$ref": "#/$defs/Int"
        },
        "exit_limit_price": {
          "$ref": "#/$defs/PrecDec"
        },
        "pick_best_route": {
          "type": "boolean",
          "description": "If pickBestRoute == true then all routes are run and the route with the\nbest price is chosen otherwise, the first succesful route is used."
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "amount_in",
        "exit_limit_price"
      ]
    },
    "MsgPlaceLimitOrder": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "receiver": {
          "type": "string"
        },
        "token_in": {
          "type": "string"
        },
        "token_out": {
          "type": "string"
        },
        "tick_index_in_to_out": {
          "type": "integer",
          "description": "Deprecated: tick_index_in_to_out will be removed in future release; limit_sell_price should be used instead."
        },
        "amount_in": {
          "$ref": "#/$defs/Int"
        },
        "order_type": {
          "type": "string"
        },
        "expiration_time": {
          "type": "integer",
          "description": "expirationTime is only valid iff orderType == GOOD_TIL_TIME."
        },
        "max_amount_out": {
          "$ref": "#/$defs/Int"
        },
        "limit_sell_price": {
          "type": "string",
          "description": "Accepts standard decimals and decimals with scientific notation (ie. 1234.23E-7)"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "amount_in",
        "max_amount_out"
      ],
      "description": "MsgPlaceLimitOrder is a copy dextypes.MsgPlaceLimitOrder with altered ExpirationTime field, it's a preferable way to pass timestamp as unixtime to contracts"
    },
    "MsgTransfer": {
      "properties": {
        "source_port": {
          "type": "string",
          "description": "the port on which the packet will be sent"
        },
        "source_channel": {
          "type": "string",
          "description": "the channel by which the packet will be sent"
        },
        "token": {
          "$ref": "#/$defs/Coin",
          "description": "the tokens to be transferred"
        },
        "sender": {
          "type": "string",
          "description": "the sender address"
        },
        "receiver": {
          "type": "string",
          "description": "the recipient address on the destination chain"
        },
        "timeout_height": {
          "$ref": "#/$defs/Height",
          "description": "Timeout height relative to the current block height.\nThe timeout is disabled when set to 0."
        },
        "timeout_timestamp": {
          "type": "integer",
          "description": "Timeout timestamp in absolute nanoseconds since unix epoch.\nThe timeout is disabled when set to 0."
        },
        "memo": {
          "type": "string"
        },
        "fee": {
          "$ref": "#/$defs/Fee"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "token",
        "timeout_height",
        "fee"
      ]
    },
    "MsgWithdrawFilledLimitOrder": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "tranche_key": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "MsgWithdrawal": {
      "properties": {
        "creator": {
          "type": "string"
        },
        "receiver": {
          "type": "string"
        },
        "token_a": {
          "type": "string"
        },
        "token_b": {
          "type": "string"
        },
        "shares_to_remove": {
          "items": {
            "$ref": "#/$defs/Int"
          },
          "type": "array"
        },
        "tick_indexes_a_to_b": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "fees": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "shares_to_remove"
      ]
    },
    "MultiHopRoute": {
      "properties": {
        "hops": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "NeutronMsg": {
      "properties": {
        "submit_tx": {
          "$ref": "#/$defs/SubmitTx"
        },
        "register_interchain_account": {
          "$ref": "#/$defs/RegisterInterchainAccount"
        },
        "register_interchain_query": {
          "$ref": "#/$defs/RegisterInterchainQuery"
        },
        "update_interchain_query": {
          "$ref": "#/$defs/UpdateInterchainQuery"
        },
        "remove_interchain_query": {
          "$ref": "#/$defs/RemoveInterchainQuery"
        },
        "ibc_transfer": {
          "$ref": "#/$defs/MsgTransfer"
        },
        "submit_admin_proposal": {
          "$ref": "#/$defs/SubmitAdminProposal"
        },
        "create_denom": {
          "$ref": "#/$defs/CreateDenom",
          "description": "Token factory types\n/ Contracts can create denoms, namespaced under the contract's address.\n/ A contract may create any number of independent sub-denoms."
        },
        "change_admin": {
          "$ref": "#/$defs/ChangeAdmin",
          "description": "/ Contracts can change the admin of a denom that they are the admin of."
        },
        "mint_tokens": {
          "$ref": "#/$defs/MintTokens",
          "description": "/ Contracts can mint native tokens for an existing factory denom\n/ that they are the admin of."
        },
        "burn_tokens": {
          "$ref": "#/$defs/BurnTokens",
          "description": "/ Contracts can burn native tokens for an existing factory denom\n/ that they are the admin of.\n/ Currently, the burn from address must be the admin contract."
        },
        "set_before_send_hook": {
          "$ref": "#/$defs/SetBeforeSendHook",
          "description": "/ Contracts can set before send hook for an existing factory denom\n/\tthat they are the admin of.\n/\tCurrently, the set before hook call should be performed from address that must be the admin contract."
        },
        "force_transfer": {
          "$ref": "#/$defs/ForceTransfer",
          "description": "/ Force transferring of a specific denom is only allowed for the creator of the denom registered during CreateDenom."
        },
        "set_denom_metadata": {
          "$ref": "#/$defs/SetDenomMetadata",
          "description": "/ Setting of metadata for a specific denom is only allowed for the admin of the denom.\n/ It allows the overwriting of the denom metadata in the bank module."
        },
        "add_schedule": {
          "$ref": "#/$defs/AddSchedule",
          "description": "Cron types"
        },
        "remove_schedule": {
          "$ref": "#/$defs/RemoveSchedule"
        },
        "resubmit_failure": {
          "$ref": "#/$defs/ResubmitFailure",
          "description": "Contractmanager types\n/ A contract that has failed acknowledgement can resubmit it"
        },
        "dex": {
          "$ref": "#/$defs/Dex",
          "description": "dex module bindings"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "description": "NeutronMsg is used like a sum type to hold one of custom Neutron messages."
    },
    "ParamChange": {
      "properties": {
        "subspace": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ParamChangeProposal": {
      "properties": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "param_changes": {
          "items": {
            "$ref": "#/$defs/ParamChange"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "title",
        "description",
        "param_changes"
      ]
    },
    "PrecDec": {
      "properties": {},
      "additionalProperties": false,
      "type": "object",
      "description": "NOTE: never use new(Dec) or else we will panic unmarshalling into the nil embedded big.Int"
    },
    "ProposalExecuteMessage": {
      "properties": {
        "message": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ProtobufAny": {
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "contentEncoding": "base64"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "type_url",
        "value"
      ],
      "description": "ProtobufAny is a hack-struct to serialize protobuf Any message into JSON object"
    },
    "RegisterInterchainAccount": {
      "properties": {
        "connection_id": {
          "type": "string"
        },
        "interchain_account_id": {
          "type": "string"
        },
        "register_fee": {
          "$ref": "#/$defs/Coins"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "connection_id",
        "interchain_account_id"
      ],
      "description": "RegisterInterchainAccount creates account on remote chain."
    },
    "RegisterInterchainQuery": {
      "properties": {
        "query_type": {
          "type": "string"
        },
        "keys": {
          "items": {
            "$ref": "#/$defs/KVKey"
          },
          "type": "array"
        },
        "transactions_filter": {
          "type": "string"
        },
        "connection_id": {
          "type": "string"
        },
        "update_period": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "query_type",
        "keys",
        "transactions_filter",
        "connection_id",
        "update_period"
      ],
      "description": "RegisterInterchainQuery creates a query for remote chain."
    },
    "RemoveInterchainQuery": {
      "properties": {
        "query_id": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "query_id"
      ]
    },
    "RemoveSchedule": {
      "properties": {
        "name": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "name"
      ],
      "description": "RemoveSchedule removes existing schedule with given name"
    },
    "ResubmitFailure": {
      "properties": {
        "failure_id": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "failure_id"
      ]
    },
    "SetBeforeSendHook": {
      "properties": {
        "denom": {
          "type": "string"
        },
        "contract_addr": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "denom",
        "contract_addr"
      ],
      "description": "SetBeforeSendHook Allowing to assign a CosmWasm contract to call with a BeforeSend hook for a specific denom is only allowed for the creator of the denom registered during CreateDenom."
    },
    "SetDenomMetadata": {
      "properties": {
        "description": {
          "type": "string"
        },
        "denom_units": {
          "items": {
            "$ref": "#/$defs/DenomUnit"
          },
          "type": "array"
        },
        "base": {
          "type": "string"
        },
        "display": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        },
        "uri": {
          "type": "string"
        },
        "uri_hash": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "description": "SetDenomMetadata is sets the denom's bank metadata"
    },
    "SubmitAdminProposal": {
      "properties": {
        "admin_proposal": {
          "$ref": "#/$defs/AdminProposal"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "admin_proposal"
      ]
    },
    "SubmitTx": {
      "properties": {
        "connection_id": {
          "type": "string"
        },
        "interchain_account_id": {
          "type": "string"
        },
        "msgs": {
          "items": {
            "$ref": "#/$defs/ProtobufAny"
          },
          "type": "array"
        },
        "memo": {
          "type": "string"
        },
        "timeout": {
          "type": "integer"
        },
        "fee": {
          "$ref": "#/$defs/Fee"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "connection_id",
        "interchain_account_id",
        "msgs",
        "memo",
        "timeout",
        "fee"
      ],
      "description": "SubmitTx submits interchain transaction on a remote chain."
    },
    "UpdateInterchainQuery": {
      "properties": {
        "query_id": {
          "type": "integer"
        },
        "new_keys": {
          "items": {
            "$ref": "#/$defs/KVKey"
          },
          "type": "array"
        },
        "new_update_period": {
          "type": "integer"
        },
        "new_transactions_filter": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
    }
  }
}